{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "http:default",
    "http:allow-fetch",
    "store:default",
    "store:allow-get",
    "store:allow-set"
  ]
}
